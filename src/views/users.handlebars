<main>
    <a href="/api/sessions/logout" class="btn btn-outline-danger w-25 mt-3">Logout</a>
    <div>
        <h1>Usuarios</h1>
        {{#each users}}
            <p>Nombre: {{this.first_name}} - {{this.last_name}}</p>
            <p>Email: {{this.email}}</p>
        {{/each}}
        <center>
        {{#if hasPrevPag}}
            <a href="/users?numPage={{prevPage}}">Anterior</a>
        {{/if}}
        <label>{{page}}</label>
        {{#if hasNextPage}}
            <a href="/users?numPage={{nextPage}}">Siguiente</a>
        {{/if}}
        </center>
    </div>
</main>
